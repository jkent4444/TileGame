package model.satellite;

public class Sun extends Satellite {
	double Gravity;
	double Mass;

	public Sun() {
		new Satellite();
		this.canOrbit = false;
	}
	
	public Sun()
	
	public void Satellite(Point position, double radius, double velocity){
		this.position = position;
		this.radius = radius;
		this.velocity = velocity;
	}
}
