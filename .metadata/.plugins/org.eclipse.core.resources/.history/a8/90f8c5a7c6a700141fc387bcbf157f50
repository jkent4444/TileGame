package model.world.objects;

public class Item {
	private String name;
	private boolean holdable;
	private boolean dropable;
	private int type;
	
	public Item() {
		
	}
	public void nullItem() {
		this.holdable = true;
		this.dropable = false;
		this.name = "null";	
		this.type = 0;
	}
	
	public int getType() {
		return type;
	}
}
