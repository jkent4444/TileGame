package model.world.objects;

import java.util.ArrayList;

public class Bag {
	private ArrayList<Item> Items;
	private int bagSize;
	
	public void setBag(int bagSize) {
		Item item;
		this.bagSize = bagSize;
		for(int i = 0; i < bagSize; i++) {
			item = new Item();
			item.nullItem();
			Items.add(item);
		}
	}
	
	public void addItem(Item item) {
		
	}
}
