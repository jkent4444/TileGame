import satellite.AsteroidBelt;
import satellite.Planet;
import satellite.Sun;


public class Solarsystem {
	private Planet[] Planets;
	private Sun[] Suns;
	private AsteroidBelt[] AsteroidBelts;
	
	public Solarsystem(int p, int s, int a) {
		for(int i = 0; i < p; i++) {
			Planet[p] = new Planet();
		}
	}
}
